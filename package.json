{
  "name": "@bumble/plop-md",
  "version": "0.3.0",
  "description": "Use an MD file to tell PlopJS how to setup your project",
  "keywords": [
    "markdown",
    "md",
    "plop",
    "plopfile",
    "plopjs",
    "typescript"
  ],
  "homepage": "https://github.com/jacksteamdev/plop-md",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacksteamdev/plop-md.git"
  },
  "author": "Jack and Amy Steam <jacksteamdev@gmail.com>",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types",
    "tsconfig.json",
    "load.js"
  ],
  "scripts": {
    "prebuild": "rm -rf dist types",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "prepublishOnly": "npm run build",
    "postpublish": "rm -rf node_modules package-lock.json && pnpm i",
    "test": "jest"
  },
  "dependencies": {
    "@types/unist": "^2.0.3",
    "fs-extra": "^8.1.0",
    "import-cwd": "^3.0.0",
    "lodash": "^4.17.15",
    "plop": "^2.6.0",
    "prettier": "^1.19.1",
    "remark": "^11.0.2",
    "ts-morph": "^6.0.3",
    "ts-node": "^8.5.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.21",
    "@types/prettier": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "6.7.2",
    "eslint-plugin-jest": "^23.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.2.0"
  },
  "peerDependencies": {
    "plop": "^2.5.3"
  }
}
